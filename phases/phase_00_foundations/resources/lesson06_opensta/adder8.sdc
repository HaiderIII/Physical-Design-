create_clock -name clk -period 5.0 [get_ports clk]
set_input_delay -clock clk -max 1.5 [get_ports {a[*] b[*] cin}]
set_input_delay -clock clk -min 0.5 [get_ports {a[*] b[*] cin}]
set_output_delay -clock clk -max 1.0 [get_ports {sum[*] cout}]
set_output_delay -clock clk -min 0.3 [get_ports {sum[*] cout}]
set_load 0.1 [get_ports {sum[*] cout}]
set_max_fanout 8 [current_design]
set_max_transition 0.5 [current_design]
