/* Simple Liberty Library for OpenSTA Tutorial */
library(simple) {
  delay_model : table_lookup;
  time_unit : "1ns";
  voltage_unit : "1V";
  current_unit : "1mA";
  pulling_resistance_unit : "1kohm";
  capacitive_load_unit(1, pf);
  
  /* Threshold definitions - REQUIRED */
  slew_lower_threshold_pct_rise : 20.0;
  slew_upper_threshold_pct_rise : 80.0;
  slew_lower_threshold_pct_fall : 20.0;
  slew_upper_threshold_pct_fall : 80.0;
  input_threshold_pct_rise : 50.0;
  input_threshold_pct_fall : 50.0;
  output_threshold_pct_rise : 50.0;
  output_threshold_pct_fall : 50.0;
  
  default_inout_pin_cap : 0.005;
  default_input_pin_cap : 0.005;
  default_output_pin_cap : 0.0;
  default_fanout_load : 1.0;
  default_max_transition : 1.5;
  
  /* Operating conditions */
  nom_process : 1.0;
  nom_voltage : 1.8;
  nom_temperature : 25.0;
  
  /* Lookup table templates */
  lu_table_template(delay_template_5x5) {
    variable_1 : input_net_transition;
    variable_2 : total_output_net_capacitance;
    index_1 ("0.01, 0.05, 0.1, 0.2, 0.5");
    index_2 ("0.01, 0.05, 0.1, 0.2, 0.5");
  }
  
  /* Simple Inverter Cell */
  cell(INV) {
    area : 1.0;
    pin(A) {
      direction : input;
      capacitance : 0.01;
    }
    pin(Y) {
      direction : output;
      max_capacitance : 0.5;
      function : "!A";
      
      timing() {
        related_pin : "A"; 
        timing_sense : negative_unate;
        
        cell_rise(delay_template_5x5) {
          values( \
            "0.05, 0.06, 0.08, 0.10, 0.15", \
            "0.06, 0.07, 0.09, 0.11, 0.16", \
            "0.08, 0.09, 0.11, 0.13, 0.18", \
            "0.10, 0.11, 0.13, 0.15, 0.20", \
            "0.15, 0.16, 0.18, 0.20, 0.25"  \
          );
        }
        
        cell_fall(delay_template_5x5) {
          values( \
            "0.04, 0.05, 0.07, 0.09, 0.14", \
            "0.05, 0.06, 0.08, 0.10, 0.15", \
            "0.07, 0.08, 0.10, 0.12, 0.17", \
            "0.09, 0.10, 0.12, 0.14, 0.19", \
            "0.14, 0.15, 0.17, 0.19, 0.24"  \
          );
        }
        
        rise_transition(delay_template_5x5) {
          values( \
            "0.02, 0.04, 0.06, 0.10, 0.20", \
            "0.03, 0.05, 0.07, 0.11, 0.21", \
            "0.05, 0.07, 0.09, 0.13, 0.23", \
            "0.08, 0.10, 0.12, 0.16, 0.26", \
            "0.15, 0.17, 0.19, 0.23, 0.33"  \
          );
        }
        
        fall_transition(delay_template_5x5) {
          values( \
            "0.02, 0.03, 0.05, 0.09, 0.19", \
            "0.03, 0.04, 0.06, 0.10, 0.20", \
            "0.04, 0.05, 0.07, 0.11, 0.21", \
            "0.07, 0.08, 0.10, 0.14, 0.24", \
            "0.14, 0.15, 0.17, 0.21, 0.31"  \
          );
        }
      }
    }
  }
}
